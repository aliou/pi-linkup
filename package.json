{
  "name": "@aliou/pi-linkup",
  "version": "0.7.1",
  "packageManager": "pnpm@10.26.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/aliou/pi-linkup"
  },
  "keywords": [
    "pi-package"
  ],
  "pi": {
    "extensions": [
      "./src/index.ts"
    ],
    "skills": [
      "./skills"
    ],
    "video": "https://assets.aliou.me/pi-extensions/demos/pi-linkup.mp4"
  },
  "exports": {
    "./tools": "./src/tools/index.ts"
  },
  "dependencies": {
    "@aliou/pi-utils-settings": "^0.2.1"
  },
  "peerDependencies": {
    "@mariozechner/pi-ai": ">=0.52.7",
    "@mariozechner/pi-coding-agent": ">=0.52.7"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "lint": "biome check",
    "format": "biome check --write",
    "prepare": "husky",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "pnpm changeset publish"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@changesets/cli": "^2.27.11",
    "@mariozechner/pi-coding-agent": "0.52.7",
    "@mariozechner/pi-tui": "0.52.7",
    "@sinclair/typebox": "^0.34.48",
    "@types/node": "^25.0.10",
    "husky": "^9.1.7",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "overrides": {
      "@mariozechner/pi-ai": "$@mariozechner/pi-coding-agent",
      "@mariozechner/pi-tui": "$@mariozechner/pi-coding-agent"
    }
  },
  "peerDependenciesMeta": {
    "@mariozechner/pi-coding-agent": {
      "optional": true
    },
    "@mariozechner/pi-ai": {
      "optional": true
    }
  }
}
